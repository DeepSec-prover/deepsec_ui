{
  "name": "deepsec_ui",
  "productName": "Deepsec UI",
  "version": "0.1.1",
  "description": "User interface for DeepSec tool (DEciding Equivalence Properties in SECurity protocols)",
  "main": "src/main.js",
  "scripts": {
    "start": "NODE_ENV=development electron src/main.js --disable-gpu",
    "check": "standard",
    "package-mac": "electron-packager . --overwrite --appCopyright=\"GPL-3.0\" --platform=darwin --arch=all --darwinDarkModeSupport=true --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . deepsec-ui --overwrite --appCopyright=\"GPL-3.0\", --asar --platform=win32 --arch=all --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=\"INRIA\" --version-string.FileDescription=\"User interface for DeepSec\" --version-string.ProductName=\"DeepSec UI\"",
    "package-linux": "electron-packager . deepsec-ui --overwrite --appCopyright=\"GPL-3.0\" --asar --platform=linux --arch=all --icon=assets/icons/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DeepSec-prover/deepsec_ui.git"
  },
  "keywords": [
    "deepsec",
    "GUI",
    "inria",
    "cryptographic protocols",
    "formal verification",
    "symbolic models",
    "privacy type properties"

  ],
  "author": "Victor YON, Vincent Cheval, Steve Kremer",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/DeepSec-prover/deepsec_ui/issues"
  },
  "homepage": "https://github.com/DeepSec-prover/deepsec_ui#readme",
  "standard": {
    "ignore": [
      "/static/"
    ]
  },
  "dependencies": {
    "electron-log": "^3.0.7"
  },
  "devDependencies": {
    "electron": "^6.0.5",
    "electron-packager": "^14.0.5",
    "standard": "^14.1.0"
  }
}
